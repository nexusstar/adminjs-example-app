{
  "name": "example",
  "version": "1.0.0",
  "description": "AdinJS example app",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build-watch": "tsc -p tsconfig.build.json -w",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "style": "prettier --check \"{src}/**/*.{{ts,js}?(x),json}\"",
    "sequelize": "./node_modules/.bin/sequelize",
    "dev": "ADMINJS_DEV_ENV=true nodemon --ext '.ts,.js' --watch ../adminjs/src --watch ./ --ignore '*.spec.js' index.js",
    "dev:watch": "nodemon --ext '.js' --watch ../adminjs/lib --watch ./ --ignore '*.spec.js' index.js",
    "dev:new": "ADMINJS_DEV_ENV=true node index.js",
    "dev:express": "EXPRESS=true nodemon --ext '.js' --watch ../adminjs/lib --watch ./ --ignore '*.spec.js' index.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@adminjs/design-system": "^2.0.4",
    "@adminjs/express": "^4.0.1",
    "@adminjs/hapi": "^4.0.1",
    "@adminjs/mongoose": "^2.0.0",
    "@adminjs/sequelize": "^2.1.0",
    "@hapi/boom": "^9.1.0",
    "@hapi/cookie": "^11.0.0",
    "@hapi/hapi": "^20.2.1",
    "@hapi/inert": "^6.0.1",
    "adminjs": "^5.5.1",
    "argon2": "^0.28.3",
    "axios": "^0.25.0",
    "cookie-parser": "^1.4.4",
    "dotenv": "^14.3.2",
    "express": "^4.17.1",
    "express-brute": "^1.0.1",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.0",
    "mongoose": "^6.1.8",
    "mysql2": "^2.3.3",
    "nodemon": "^2.0.15",
    "ora": "^6.0.1",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.14.1",
    "sequelize-cli": "^6.4.1",
    "typeorm": "^0.2.29"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.0.0-alpha.5",
    "@types/express": "^4.17.9",
    "@types/flat": "^5.0.1",
    "@types/node": "^17.0.12",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-disable": "^2.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.2.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  }
}
